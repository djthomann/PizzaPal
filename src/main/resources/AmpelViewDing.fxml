<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <children>
      <HBox alignment="CENTER">
         <children>
            <VBox style="-fx-background-color: darkgrey;">
               <children>
                  <Circle fx:id="rot" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
                  <Circle fx:id="gelb" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
                  <Circle fx:id="gruen" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
               </children>
            </VBox>
            </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="tick" mnemonicParsing="false" onAction="#tick" text="tick">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="reset" mnemonicParsing="false" onAction="#reset" text="reset">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
